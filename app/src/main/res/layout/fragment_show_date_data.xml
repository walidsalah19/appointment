<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".doctor.show_date_data">
    <androidx.core.widget.NestedScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/white"
        >
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        >
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="16dp"
        android:textColor="@color/black"
        android:text="الاسم "
        android:layout_margin="10dp"
        >
    </TextView>
    <EditText
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textColor="@color/black"
        android:background="@drawable/edittext_shaps"
        android:textSize="16dp"
        android:hint="اسم "
        android:id="@+id/doctor_show_date_name"
        android:enabled="false"
        android:textColorHint="@color/black"
        android:layout_marginRight="10dp"
        android:layout_marginLeft="10dp"
        android:padding="10dp"
        >
    </EditText>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="16dp"
        android:textColor="@color/black"
        android:text="العمر"
        android:layout_margin="10dp"
        >
    </TextView>
    <EditText
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textColor="@color/black"
        android:background="@drawable/edittext_shaps"
        android:textSize="16dp"
        android:id="@+id/doctor_show_date_age"
        android:hint="العمر"
        android:enabled="false"
        android:textColorHint="@color/black"
        android:layout_marginRight="10dp"
        android:layout_marginLeft="10dp"
        android:padding="10dp"
        >
    </EditText>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="16dp"
        android:textColor="@color/black"
        android:text="رقم الملف"
        android:layout_margin="10dp"
        >
    </TextView>
    <EditText
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textColor="@color/black"
        android:background="@drawable/edittext_shaps"
        android:textSize="16dp"
        android:id="@+id/doctor_show_date_filenum"
        android:hint="1"
        android:enabled="false"
        android:inputType="number"
        android:textColorHint="@color/black"
        android:layout_marginRight="10dp"
        android:layout_marginLeft="10dp"
        android:padding="10dp"
        >
    </EditText>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="16dp"
        android:textColor="@color/black"
        android:text="العيادة"
        android:layout_margin="10dp"
        >
    </TextView>
    <EditText
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textColor="@color/black"
        android:background="@drawable/edittext_shaps"
        android:textSize="16dp"
        android:id="@+id/doctor_show_date_clinic"
        android:hint=" العيادة"
        android:enabled="false"
        android:inputType="text"
        android:textColorHint="@color/black"
        android:layout_marginRight="10dp"
        android:layout_marginLeft="10dp"
        android:padding="10dp"
        >
    </EditText>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="16dp"
            android:textColor="@color/black"
            android:text="تاريخ الحجز"
            android:layout_margin="10dp"
            >
        </TextView>
        <EditText
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textColor="@color/black"
            android:background="@drawable/edittext_shaps"
            android:textSize="16dp"
            android:hint="1/9/2022"
            android:id="@+id/doctor_show_date_bookindate"
            android:inputType="text"
            android:enabled="false"
            android:textColorHint="@color/black"
            android:layout_marginRight="10dp"
            android:layout_marginLeft="10dp"
            android:padding="10dp"
            >
        </EditText>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="16dp"
            android:textColor="@color/black"
            android:text="تاريخ الموعد"
            android:layout_margin="10dp"
            >
        </TextView>
        <EditText
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textColor="@color/black"
            android:background="@drawable/edittext_shaps"
            android:textSize="16dp"
            android:hint="1/10/2020"
            android:id="@+id/doctor_show_date_date"
            android:inputType="text"
            android:enabled="false"
            android:textColorHint="@color/black"
            android:layout_marginRight="10dp"
            android:layout_marginLeft="10dp"
            android:padding="10dp"
            >
        </EditText>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="16dp"
            android:textColor="@color/black"
            android:text="الوقت"
            android:layout_margin="10dp"
            >
        </TextView>
        <EditText
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textColor="@color/black"
            android:background="@drawable/edittext_shaps"
            android:textSize="16dp"
            android:hint="6.55"
            android:id="@+id/doctor_show_date_time"
            android:inputType="datetime"
            android:enabled="false"
            android:textColorHint="@color/black"
            android:layout_marginRight="10dp"
            android:layout_marginLeft="10dp"
            android:padding="10dp"
            >
        </EditText>
    </LinearLayout>
    </androidx.core.widget.NestedScrollView>
</FrameLayout>