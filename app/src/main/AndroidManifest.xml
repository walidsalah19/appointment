<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.booking">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Booking">
        <activity android:name=".user_access.splash"></activity>
        <activity android:name=".hospital.hospital_activity" />
        <activity android:name=".hospital.hospital_info" />
        <activity android:name=".user_access.forget_password" />
        <activity android:name=".user.main_user" />
        <activity android:name=".doctor.doctor_main_activity" />
        <activity android:name=".user_access.registration" />
        <activity android:name=".user.user_data_profile" />
        <activity android:name=".user_access.login">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />
    </application>

</manifest>